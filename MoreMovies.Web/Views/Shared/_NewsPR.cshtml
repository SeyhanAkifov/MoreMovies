@model ICollection<NewsViewModel>

<section id="news" class="col-md-12 m-lg-auto">
    <article class="head">
        <h3>NEWS</h3>
        <p class="text-right"><a asp-controller="News" asp-action="All">See all</a></p>
    </article>
    @foreach (var item in Model)
    {
        @await Html.PartialAsync("../News/News", item)

    }
</section>
